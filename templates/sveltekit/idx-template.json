{
  "name": "SvelteKit",
  "description": "web development, streamlined",
  "categories": ["Web app", "Svelte"],
  "icon": "https://www.gstatic.com/monospace/231115/logo_svelte.svg",
  "publisher": "Julien Martel",
  "params": [
    {
      "id": "template",
      "name": "Template",
      "type": "enum",
      "default": "demo",
      "options": {
        "demo": "Demo app",
        "minimal": "Minimal app",
        "library": "Library project"
      },
      "required": true
    },
    {
      "id": "types",
      "name": "Types",
      "type": "enum",
      "default": "checkjs",
      "options": {
        "jsdoc": "JSDoc comments",
        "ts": "TypeScript syntax",
        "null": "None"
      },
      "required": true
    },
    {
      "id": "prettier",
      "name": "Use Prettier",
      "type": "boolean",
      "default": true,
      "required": true
    },
    {
      "id": "eslint",
      "name": "Use ESLint",
      "type": "boolean",
      "default": true,
      "required": true
    },
    {
      "id": "vitest",
      "name": "Use Vitest",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "playwright",
      "name": "Use Playwright",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "tailwindcss",
      "name": "Use Tailwind CSS",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "drizzle",
      "name": "Use Drizzle",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "lucia",
      "name": "Use Lucia",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "mdsvex",
      "name": "Use MDsveX",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "paraglide",
      "name": "Use Paraglide",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "storybook",
      "name": "Use Storybook",
      "type": "boolean",
      "default": false,
      "required": true
    }
  ]
}
